<div id="all-fpr-forms" v-cloak>
  <div class="container">
    <h1>Funding Proposal Requests</h1>
    <div class="row mb-3">
      <div class="col-sm-12">
        <h4>Saved Drafts</h4>
      </div>
      <div class="col-sm-12">
        <ul>
          <li v-for="item in forms.draft">
            <a v-bind:href="'/form/'+item.id">{{ item.projectName }}</a>
          </li>
        </ul>
      </div>
    </div>
    <div class="row mb-3">
      <div class="col-sm-12">
        <h4>Pending Approval</h4>
      </div>
      <div class="col-sm-12">
        <ul>
          <li v-for="item in forms.pending">
            <a v-bind:href="'/form/'+item.id">{{ item.projectName }}</a>
          </li>
        </ul>
      </div>
    </div>
    <div class="row mb-3">
      <div class="col-sm-12">
        <h4>Approved Proposals</h4>
      </div>
      <div class="col-sm-12">
        <ul>
          <li v-for="item in forms.approved">
            <a v-bind:href="'/form/'+item.id">{{ item.projectName }}</a>
          </li>
        </ul>
      </div>
    </div>
  </div>
</div>
<%- /* Expose locals as `window.SAILS_LOCALS` :: */ exposeLocalsToBrowser() %>
